{
  "version": 1,
  "serviceConfiguration": {
    "apiName": "Libgloss",
    "serviceName": "AppSync",
    "defaultAuthType": {
      "mode": "API_KEY",
      "keyDescription": "api key description",
      "expirationTime": 30
    },
    "additionalAuthTypes": [
      {
        "mode": "AWS_IAM"
      }
    ],
    "conflictResolution": {
      "defaultResolutionStrategy": {
        "type": "AUTOMERGE"
      },
      "perModelResolutionStrategy": [
        {
          "resolutionStrategy": {
            "type": "AUTOMERGE"
          },
          "entityName": "WishListItem"
        },
        {
          "resolutionStrategy": {
            "type": "AUTOMERGE"
          },
          "entityName": "UserBooks"
        },
        {
          "resolutionStrategy": {
            "type": "AUTOMERGE"
          },
          "entityName": "TrackingListItem"
        },
        {
          "resolutionStrategy": {
            "type": "AUTOMERGE"
          },
          "entityName": "Users"
        },
        {
          "resolutionStrategy": {
            "type": "AUTOMERGE"
          },
          "entityName": "Seller"
        },
        {
          "resolutionStrategy": {
            "type": "AUTOMERGE"
          },
          "entityName": "ListWish"
        },
        {
          "resolutionStrategy": {
            "type": "AUTOMERGE"
          },
          "entityName": "Book"
        },
        {
          "resolutionStrategy": {
            "type": "AUTOMERGE"
          },
          "entityName": "Settings"
        }
      ]
    }
  }
}